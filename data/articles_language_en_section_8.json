            "url": "http://deschide.md/en/news/sport/6034/simona-halep-has-been-dumped-from-the-australian-open-in-the-first-round-video.htm",
            "languageData": {
                "id": 1,
                "name": "English",
                "code": "en",
                "RFC3066bis": "en-US"
            },
            "status": "Y",
            "isLocked": false,
            "language": "en",
            "publication": {
                "id": 1,
                "name": "Deschide Știrea"
            },
            "issue": {
                "number": 1,
                "language": "en",
                "title": "news",
                "workflowStatus": "Y",
                "shortName": "news"
            },
            "section": {
                "language": "en",
                "number": 8,
